	"scripts": {
		"prestart": "npm run lint && npm run test && npm run format",
		"prebuild": "npm run prestart",
		"start": "NODE_ENV=development webpack-dev-server",
		"build": "rm -rf build/ && NODE_ENV=production webpack --progress",
		"deploy": "echo 'TODO: Write the deploy script.'",
		"lint": "npm run lint:eslint && npm run lint:stylelint",
		"lint:eslint": "eslint 'src/**/*.{js,jsx}'",
		"lint:stylelint": "stylelint 'src/**/*.css'",
		"test": "jest",
		"test:watch": "jest --watch --verbose",
		"test:coverage": "jest --watch --coverage",
		"format": "npm run format:stylelint && npm run format:prettier",
		"format:prettier": "prettier --loglevel='silent' --write 'src/**/*.{js,jsx,json}'",
		"format:stylelint": "stylelint --fix 'src/**/*.css'"
	},
